    {% block javascripts %}
    <script type="text/javascript" src="{{ asset('assets/js/app.js') }}"></script>
    {% endblock %}
</head>
<body>
    <h1>{% block title_h1 %}Titre de la page{% endblock %}</h1>

    {% block contenu_1 %}
