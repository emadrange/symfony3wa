{% extends "TroiswaBackBundle::layout-admin.html.twig" %}

{% block title %}Editer la marque{% endblock %}

{% block h1 %}<i class="fa fa-certificate"></i> Editer la marque{% endblock %}

{% block breadcrumb %}
    {{ parent() }}
    <li>
        <a href="{{ path('troiswa_back_marque_list') }}"><i class="fa fa-certificate"></i> Lister les marques</a>
    </li>
    <li class="active">
        <i class="fa fa-certificate"></i> Editer la marque
    </li>
{% endblock %}

{% block content %}
    <div class="col-lg-8">
    {{ form_start(formMarque) }}

    {{ form_row(formMarque.title) }}
    {{ form_row(formMarque.description) }}
    {{ form_row(formMarque.products) }}
    {{ form_row(formMarque.logo.logofile) }}
    {{ form_row(formMarque.logo.legende) }}

    {{ form_end(formMarque) }}
    </div>
{% endblock %}